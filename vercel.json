{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "build/src/routes/image.route.js"
        },
        {
            "src": "/(.*)",
            "dest": "build/src/routes/post.route.js"
        }
    ]
}